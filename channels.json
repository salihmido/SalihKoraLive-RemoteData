data class RootResponse(
    val version: Int,
    val last_update: String,
    val channels: List<Channel>
)

data class Channel(
    val id: Int,
    val name: String,
    val category_id: Int,
    val logo: String,
    val streams: List<Stream>,
    val backup_stream: String,
    val status: String
)

data class Stream(
    val quality: String,
    val url: String
)
